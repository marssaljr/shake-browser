{
    "manifest_version": 3,
    "name": "Floating Divs",
    "version": "1.0",
    "description": "Makes all divs float around the mouse cursor.",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "styles.css"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}